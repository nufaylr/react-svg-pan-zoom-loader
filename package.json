{
  "name": "react-svg-pan-zoom-loader",
  "version": "2.0.0",
  "description": "This package allows us to load a SVG file remotely into react-svg-pan-zoom component.",
  "main": "./build-commonjs/index.js",
  "module": "./build-es/index.js",
  "types": "./build-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./build-types/index.d.ts",
      "import": "./build-commonjs/index.js",
      "require": "./build-es/index.js",
      "default": "./build-es/index.js"
    }
  },
  "scripts": {
    "build:commonjs": "tsc -b ./tsconfig.cjs.json",
    "build:es": "tsc -b ./tsconfig.esm.json",
    "build:type": "tsc -b ./tsconfig.types.json",
    "clean": "del build-*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "nufaylr",
  "contributors": [
    "chrvadala"
  ],
  "license": "MIT",
  "keywords": [
    "react-svg-pan-zoom",
    "svg-file",
    "reactjs svg file"
  ],
  "files": [
    "*.md",
    "build-*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrvadala/react-svg-pan-zoom-loader.git"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-svg-pan-zoom": "^3.3.5",
    "del": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react-svg-pan-zoom": "^3.12.1",
    "react-svgmt": "^2.0.2"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0 || ^17.0.0 "
  }
}
