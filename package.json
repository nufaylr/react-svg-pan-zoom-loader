{
  "name": "react-svg-pan-zoom-loader",
  "version": "next",
  "description": "This package allows us to load a SVG file remotely into react-svg-pan-zoom component.",
  "main": "./build-commonjs/index.js",
  "module": "./build-es/index.js",
  "types": "./build-types/index.d.ts",
  "exports": {
    ".": {
      "types": "./build-types/index.d.ts",
      "import": "./build-commonjs/index.js",
      "require": "./build-es/index.js",
      "default": "./build-es/index.js"
    }
  },
  "scripts": {
    "build:commonjs": "tsc -b ./tsconfig.cjs.json",
    "build:es": "tsc -b ./tsconfig.esm.json",
    "build:type": "tsc -b ./tsconfig.types.json",
    "build": "npm run lint && npm run clean && npm run build:commonjs && npm run build:es && npm run build:type",
    "clean": "npx rimraf build-*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
		"lint": "tsc"
  },
  "author": "nufaylr",
  "contributors": [
    "chrvadala"
  ],
  "license": "MIT",
  "keywords": [
    "react-svg-pan-zoom",
    "svg-file",
    "reactjs svg file"
  ],
  "files": [
    "*.md",
    "build-*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrvadala/react-svg-pan-zoom-loader.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-webpack5": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.2.0",
    "@types/react-svg-pan-zoom": "^3.3.5",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.7",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "react-svg-pan-zoom": "^3.12.1",
    "react-svgmt": "^2.0.2"
  },
  "peerDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0 || ^17.0.0 "
  }
}
